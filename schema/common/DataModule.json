{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "./common/DataModule.json",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "name": {
      "type": "string"
    },
    "displayName": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "tags": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "parameters": {
      "type": "array",
      "items": {
        "$ref": "./Parameter.json"
      }
    }
  }
}